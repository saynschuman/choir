<h1>Хористы</h1> 
  <table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">admin</th>
      <th scope="col">email</th>
      <th scope="col">link</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <th scope="row">
        <%= user.id %>
      </th>
      <td>
        <%= user.admin %>
      </td>
      <td>
        <%= user.email %>
      </td>
      <td>
        <%= link_to 'перейти', user %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>
